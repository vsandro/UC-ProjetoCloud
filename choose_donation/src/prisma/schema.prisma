generator client {
  provider      = "prisma-client-js"  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Stores {
  id   String @id @default(uuid())
  name String
  address String
  latitude Int
  longitude Int
  id_city Int
  active Boolean @default(true)

  @@map("stores")
}

